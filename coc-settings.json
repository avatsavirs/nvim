{
  "suggest.noselect": false,
  "explorer.width": 50,
  "explorer.quitOnOpen": false,
  "explorer.git.showIgnored": false,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings.global": {
    "<C-v>": "open:vsplit"
  },
  "explorer.position": "floating",
  "coc.preferences.formatOnSaveFiletypes": ["css", "markdown", "javascript", "html", "graphql", "json", "cpp", "json", "javascriptreact"],
  "typescript.preferences.quoteStyle": "single",
  "javascript.preferences.quoteStyle": "single",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "javascript.updateImportsOnFileMove.enable": true,
  "javascript.validate.enable": false,
  "typescript.updateImportsOnFileMove.enable": true,
  "diagnostic.errorSign": "✖",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "•",
  "diagnostic.virtualText": true,
  "diagnostic.messageTarget": "echo",
}
